{
    "manifest_version": 3,
    "name": "Hide Youtube Vocal Search",
    "version": "1.5",
    "description": "This extension hides the vocal search button on YouTube.",
    "icons": {
        "32": "icon-32.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "content_script.js"
            ],
            "css": [
                "style.css"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "https://www.youtube.com/*"
    ]
}
